{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8"
  },
  "python.formatting.provider": "none",
  "search.useIgnoreFiles": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/.next": true,
    ".venv": true,
    "myenv": true,
    "**.log": true,
    "**/public/web": true,
    "**/comfyui-fork/models": true
  }
}
